 });\r\n    },\r\n    FETCH_ITEM({ commit }, id) {\r\n        fetchCommentItem(id)\r\n        .then(({ data }) => {\r\n            commit('SET_ITEM', data);\r\n        })\r\n        .catch( error => {\r\n            console.log(error)\r\n        });\r\n    },\r\n    FETCH_LIST({ commit }, pageName ){\r\n        fetchList(pageName)\r\n        .then(({ data }) => commit('SET_LIST', data))\r\n        .catch(error => console.log(error));\r\n    }\r\n}"]}]}